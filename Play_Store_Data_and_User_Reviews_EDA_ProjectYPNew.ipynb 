        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Assuming df2 is your DataFrame containing 'Sentiment' column\n",
        "counts = df2['Sentiment'].value_counts()\n",
        "labels = 'Positive Reviews', 'Negative Reviews', 'Neutral Reviews'\n",
        "\n",
        "# Set the font size and figure size using matplotlib's parameters\n",
        "plt.rcParams['font.size'] = 12\n",
        "plt.rcParams['figure.figsize'] = (8, 8)\n",
        "\n",
        "# Create the pie chart with specified attributes\n",
        "plt.pie(counts, labels=labels, explode=[0, 0.05, 0.005], shadow=True, autopct=\"%.2f%%\")\n",
        "\n",
        "# Set the title for the pie chart\n",
        "plt.title('A Pie Chart Representing Percentage of Review Sentiments', fontsize=20)\n",
        "\n",
        "# Add a legend to label the different segments\n",
        "plt.legend()\n",
        "\n",
        "# Show the pie chart\n",
