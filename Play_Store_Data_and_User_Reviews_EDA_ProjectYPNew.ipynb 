    {
      "cell_type": "code",
      "source": [
        "# Convert 'Reviews' column to numeric values\n",
        "df1['Reviews'] = pd.to_numeric(df1['Reviews'], errors='coerce')\n",
        "\n",
        "# Select the top 10 apps with the highest number of reviews\n",
        "top_10_review_app = df1.sort_values(by=['Reviews'], ascending=False).head(10).reset_index(drop=True)\n",
        "\n",
        "# Create a horizontal bar plot using seaborn\n",
        "sns.barplot(y='App', x='Reviews', data=top_10_review_app, palette='rainbow', orient='h')\n",
        "\n",
        "# Set the title of the plot\n",
        "plt.title('Top 10 Most Reviewed Apps')\n",
        "\n",
        "# Set the x-axis label\n",
        "plt.xlabel('Number of Reviews')\n",
        "\n",
        "# Set the y-axis label\n",
        "plt.ylabel('App Name')\n",
        "\n",
        "# Show the plot\n",
        "plt.show()\n"
      ],
      "metadata": {
        "id": "0WiBhNZxQChP"
      },
